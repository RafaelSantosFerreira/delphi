procedure TNfse.reloadLnfseItens;
var
	dao: NfseItensDAO;
	lTemp: TList<TNfseItens>;
	i, s: Integer;
begin
	dao := nil;
	try
		dao := NfseItensDAO.Create(sEmpresaLogada, sFilialLogada);
		lTemp := dao.Seleciona('[a584_cod_nota] = :p', [sCodigo]);
		_lnfseItens.clear;
		if lTemp <> nil then
		begin
			i := 0;
			s := lTemp.Count;
			while i < s do
			begin
				_lnfseItens.add(lTemp[i]);
				i := i + 1;
			end;
			lTemp.Free;
		end;
		dao.Free;
	except
		if dao <> nil then
			dao.Free;
	end;
end;

