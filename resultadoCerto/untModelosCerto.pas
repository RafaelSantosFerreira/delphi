procedure TNotaServico.reloadLNotasServicoItens;
var
	dao: ItensNotaDeServicoDAO;
	lTemp: TList<TItensNotaDeServico>;
	i, s: Integer;
begin
	dao := nil;
	try
		dao := ItensNotaDeServicoDAO.Create(sEmpresaLogada, sFilialLogada);
		lTemp := dao.Seleciona('[a226_cod_nota_servico] = :p', [sCodigo]);
		_lNotasServicoItens.clear;
		if lTemp <> nil then
		begin
			i := 0;
			s := lTemp.Count;
			while i < s do
			begin
				_lNotasServicoItens.add(lTemp[i]);
				i := i + 1;
			end;
			lTemp.Free;
		end;
		dao.Free;
	except
		if dao <> nil then
			dao.Free;
	end;
end;

